FACTORYDESCRIPTION=This is all done within Manual Options. Converts DD5.1 or DTS5.1 to 6 individual audio files via Filter Complex
NOTIFYDIRECTORY=
OUTPUTDIRECTORY=/video/
VIDEOCODECS=
VIDEOWRAPPER=
VIDEOFRAMERATE=
VIDEOSIZE=
VIDEOFILTERS=
VIDEOPIXFORMAT=
ASPECT=
VIDEOBITRATE=
MATCHMINMAXBITRATE=False
VIDEOPROFILE=
VIDEOPROFILELEVEL=
VIDEOPRESET=
VIDEOSTREAMID=
GROUPPICSIZE=
BFRAMES=
FRAMESTRATEGY=
VIDEOFORMAT=
FORCEFORMAT=
ENCODELENGTH=
STARTTIMEOFFSET=
FLAGS=
FLAGS2=
SUBTITLECODECS=
REMOVEA53CC=False
AUDIOCODECS=
AUDIOBITRATE=
AUDIOSAMPLERATE=
AUDIOFILEEXTENSION=
AUDIOFILTERS=
AUDIOCHANNELS=
AUDIOSTREAMID=
MANUALOPTIONSOUTPUT=-filter_complex "channelsplit=channel_layout=5.1[FL][FR][FC][LFE][BL][BR]" -map "[FL]" {outdir}{stem}front_left.wav -map "[FR]" {outdir}{stem}front_right.wav -map "[FC]" {outdir}{stem}front_center.wav -map "[LFE]" {outdir}{stem}lfe.wav -map "[BL]" {outdir}{stem}back_left.wav -map "[BR]" {outdir}{stem}back_right.wav
MANUALOPTIONSINPUT=
FORCEFORMATINPUTVIDEO=
FORCEFORMATINPUTAUDIO=
STREAMINPUTVIDEO=
STREAMINPUTAUDIO=
STREAMRTMPURL=
STREAMKEY=
STREAMAUTHMODE=
STREAMUSERNAME=
STREAMPASSWORD=
INCLUDETQS=True
LOWLATENCYINPUT=False
AUTOMAPAV=False
TQSSIZE=512
STREAMMGRMODE=Off
READFILESREALTIME=False
DISABLEVIDEO=False
DISABLEAUDIO=False
DISABLESUBS=False
DISABLEDATA=False
RECORDINPUT=
RECORDFOLDER=
RECORDFILENAME=
ENABLEFACTORY=True
DELETECONVERSIONLOGS=True
DELETESOURCE=True
MULTIOUTPUT=True
